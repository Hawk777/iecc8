<Grid xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		x:Class="Iecc8.UI.Equipment.AutoButton"
		Width="16"
		Height="16"
		Grid.ColumnSpan="2">
	<Grid.Resources>
		<ResourceDictionary Source="pack://application:,,,/iecc8;component/UI/Common/Resources.xaml"/>
	</Grid.Resources>
	<Grid.ColumnDefinitions>
		<ColumnDefinition Width="8"/>
		<ColumnDefinition Width="8"/>
	</Grid.ColumnDefinitions>
	<Grid.RowDefinitions>
		<RowDefinition Height="16"/>
	</Grid.RowDefinitions>
	<Grid.Background>
		<StaticResource ResourceKey="BG"/>
	</Grid.Background>
	<TextBlock Text="A" Foreground="{StaticResource SignalAutoButton}" TextAlignment="Center"/>
	<Ellipse x:Name="Roundel" Grid.Column="1" Fill="{StaticResource SignalAutoButton}" Width="8" Height="8"/>
</Grid>
